<div class="text-center p-5">
  <h1 class="jumbotron-heading">UAlbany History</h1>

  <p class="lead">Discover events from the school's history.</p>

</div>
<div class="card-columns">
<% subject_options.sort.each do |name, url| %>

	<a href="/history/catalog?f[subjects][]=<%= name %>">
		<div class="card subject-cards">
		  <img class="card-img-top" src="<%= url %>?file=thumbnail">
		  <div class="card-body text-center">
		  	<h5 class="card-title"><%= name %></h5>
		  </div>
		</div>
	</a>

<% end %>
</div>